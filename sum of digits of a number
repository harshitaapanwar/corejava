import java.util.Scanner;
class Check
{
    public static void main(String[] args) 
    {
    Scanner s=new Scanner(System.in);
    System.out.print("Enter a number=");
    int a=s.nextInt();
    int r,sum=0;
    while(a>0)
    {
    r=a%10;
    sum=sum+r;
    a=a/10;
    }
    System.out.print(sum);
    }
}
