import java.util.Scanner;
class factorial
{
    public static void main(String[] args) 
    {
    Scanner s=new Scanner(System.in);
    System.out.print("Enter a number=");
    int n=s.nextInt();
    int i,fact=1;
    for(i=1;i<=n;i++)
    {
       fact =fact*i;
    }
    System.out.print("factorial of a number="+fact);
    }
}
